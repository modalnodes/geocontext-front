  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@modalnodes/mn-geo": [
        "dist/mn-geo"
      ],
      "@modalnodes/mn-map": [
        "dist/mn-map"
      ],
      "@modalnodes/mn-mapgl": [
        "dist/mn-mapgl"
      ],
      "@geocontext/gcx-core": [
        "dist/gcx-core"
      ],
      "ohm-core": [
        "dist/ohm-core"
      ],
      "c3d-core": [
        "dist/c3d-core"
      ],
      "chcx-main": [
        "dist/chcx-main"
      ]
    }
  }
